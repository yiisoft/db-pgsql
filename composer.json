{
    "name": "yiisoft/db-pgsql",
    "type": "library",
    "description": "Yii DataBase PostgreSQL Extension",
    "keywords": [
        "yii",
        "pgsql"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/db-pgsql",
        "issues": "https://github.com/yiisoft/db-pgsql/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "yiisoft/arrays": "^3.0@dev",
        "yiisoft/db": "^3.0@dev",
        "yiisoft/json": "^3.0@dev",
        "yiisoft/strings": "^3.0@dev"
    },
    "require-dev": {
        "infection/infection": "^0.17",
        "phan/phan": "^3.0",
        "phpunit/phpunit": "^9.3",
        "yiisoft/aliases": "^1.0",
        "yiisoft/cache": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Db\\Pgsql\\": "src",
            "Yiisoft\\Db\\Pgsql\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
